{
	"luci-app-frpc": {
		"description": "Grant access to LuCI app frpc",
		"read": {
			"file": {
				"/etc/passwd": ["read"],
				"/etc/group": ["read"],
				"/sbin/logread": [ "exec" ],
				"/usr/bin/frpc": [ "exec" ],
				"/usr/libexec/frpc-call": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": ["frpc"]
		},
		"write": {
			"uci": ["frpc"]
		}
	}
}

