{
	"luci-app-frps": {
		"description": "Grant access to LuCI app frps",
		"read": {
			"file": {
				"/etc/group": [ "read" ],
				"/etc/group": ["read"],
				"/sbin/logread": [ "exec" ],
				"/usr/bin/frps": [ "exec" ],
				"/usr/libexec/frps-call": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": ["frps"]
		},
		"write": {
			"uci": ["frps"]
		}
	}
}

